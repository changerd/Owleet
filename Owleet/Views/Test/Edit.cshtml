@model Owleet.Models.Test

@{
    ViewData["Title"] = "Edit test";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div id="modal-placeholder"></div>
<h4>Test</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        @await Html.PartialAsync("_Input", Model)
    </div>
</div>
<div id="partial" data-url="@Url.RouteUrl(new {Controller = "Question",Action="Index", testId = Model?.Id})">
    @await Html.PartialAsync("~/Views/Question/Index.cshtml", Model)
</div>
<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
